<properties>

    <entry key="payment-cancelled">User "{0}" cancelled "{1}" payment of {2}.</entry>
    <entry key="payment-expired">Transfer of {0} via "{1}" of user "{2}" expired.</entry>       
    <entry key="payment-acknowledged">Transfer of {0} via "{1}" of user "{2}" acknowledged.</entry>
    <entry key="payment-ignored-user-unknown">{0} transaction {1} of {2} ignored: user "{3}" unknown.</entry>
    
